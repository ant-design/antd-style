{
  "include": ["src", ".dumi/**/*", ".dumirc.ts", "*.ts"],
  "compilerOptions": {
    "strict": true,
    "declaration": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "jsx": "react-jsx",
    "lib": ["DOM", "ESNext"],
    "baseUrl": ".",
    "paths": {
      "@@/*": [".dumi/tmp/*"],
      "@/*": ["src/*"],
      "antd-style": ["src"],
      "antd-style/*": ["src/*", "*"]
    }
  }
}
